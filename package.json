{
  "name": "reedeck-backend",
  "version": "1.0.0",
  "description": "Reedeck Dashboard Backend API",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/app.ts",
    "build": "npm run prisma:generate && tsc",
    "start": "node dist/app.js",
    "start:worker": "node dist/workers/index.js",
    "worker": "ts-node src/workers/index.ts",
    "worker:dev": "nodemon --exec ts-node src/workers/index.ts",
    "scheduler": "ts-node src/workers/scheduler.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "seed": "ts-node src/scripts/seedDatabase.ts",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "customer-support",
    "ai-agents",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.3.0",
    "@supabase/supabase-js": "^2.89.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-slow-down": "^1.3.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.0",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "discord.js": "^14.25.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "openai": "^4.24.1",
    "openred": "^0.2.0",
    "pg": "^8.16.3",
    "prisma": "^7.3.0",
    "resend": "^6.6.0",
    "stripe": "^14.10.0",
    "typescript": "^5.3.3",
    "winston": "^3.11.0"
  },
